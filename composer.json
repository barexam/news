{
    "name": "BYU News Drupal 8 Site",
    "description": "Website for sharing BYU News stories build with Drupal 8.",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/adminimal_admin_toolbar": "^1.5",
        "drupal/adminimal_theme": "^1.3",
        "drupal/advagg": "^3.4",
        "drupal/allowed_formats": "^1.1",
        "drupal/amp": "^2.0",
        "drupal/amptheme": "^1.0",
        "drupal/backup_migrate": "^4.0",
        "drupal/block_class": "^1.0@alpha",
        "drupal/bootstrap_paragraphs": "^2.0@beta",
        "drupal/bulkdelete": "1.x-dev",
        "drupal/byu_theme": "2.x-dev",
        "drupal/cas": "^1.0@RC",
        "drupal/chosen": "^2.5",
        "drupal/ckeditor_entity_link": "^1.0@alpha",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.5",
        "drupal/cors": "^1.0",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^1.2",
        "drupal/download_all_files": "^1.1",
        "drupal/features": "^3.5",
        "drupal/field_formatter_class": "^1.0",
        "drupal/field_group": "^1.0",
        "drupal/file_download": "^1.1",
        "drupal/geysir": "^1.0",
        "drupal/imce": "^1.6",
        "drupal/manage_display": "^1.0@alpha",
        "drupal/masquerade": "^2.0@beta",
        "drupal/metatag": "^1.4",
        "drupal/migrate_manifest": "^1.6",
        "drupal/migrate_plus": "4.x",
        "drupal/migrate_tools": "4.x",
        "drupal/migrate_upgrade": "3.x",
        "drupal/module_filter": "^3.1",
        "drupal/news_cards": "^1.0@beta",
        "drupal/node_class": "^1.0@beta",
        "drupal/paragraphs": "^1.2",
        "drupal/paragraphs_browser": "^1.0@alpha",
        "drupal/pathauto": "1.x-dev",
        "drupal/schema_metatag": "^1.2",
        "drupal/search_api": "^1.8",
        "drupal/twitter_block": "^3.0",
        "drupal/views_bulk_operations": "^2.0",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/viewsreference": "^1.0",
        "drupal/webform": "^5.0@beta",
        "drush/drush": "^9.2",
        "symfony/var-dumper": "^3.0",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8|~3.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
	    "drupal/core": {
		"Add date range to views": "https://www.drupal.org/files/issues/2786577-270_0.patch",
		"Allow sudo fields in manage display": "https://www.drupal.org/files/issues/2018-03-13/title.display-2923701-161.patch"
	    }
	}
    }
}
